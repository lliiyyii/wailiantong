            android:layout_height="match_parent">
            <!--布局上面出现白边-->
            <!--android:layout_centerHorizontal="true"-->
            <!--android:scaleType="centerInside"-->
            <!--android:adjustViewBounds="true"-->
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:src="@mipmap/home_red_bg"
                android:layout_centerHorizontal="true"
                android:scaleType="centerInside"
                android:adjustViewBounds="true" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/app_name"
                android:textColor="@color/white"
                android:textSize="18dp"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="33.5dp"/>

            <RelativeLayout
                android:layout_centerHorizontal="true"
                android:layout_marginTop="190dp"
                android:layout_width="350dp"
                android:layout_height="360dp"
                android:background="@color/white">
                <View
                    android:layout_width="1px"
                    android:layout_height="match_parent"
                    android:background="@color/text_gray"
                    android:layout_centerHorizontal="true">

                </View>

                <RelativeLayout

                    android:clickable="true"
                    android:id="@+id/home_test_1"
                    android:layout_width="175dp"
                    android:layout_height="120dp">
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1px"
                        android:layout_alignParentBottom="true">
                    </View>
                    <ImageView
                        android:id="@+id/home_img1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/home_item_imgtop"
                        android:layout_centerHorizontal="true"
                        android:src="@mipmap/home_icon1"
                        />
                    <TextView
                        android:id="@+id/home_text11"
                        android:layout_below="@id/home_img1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/home_item_imgbottom"
                        android:text="真人与证件照对比"
                        android:textColor="@color/text_black"
                        android:layout_centerHorizontal="true"
                        />
                    <TextView
                        android:id="@+id/home_text12"
                        android:layout_below="@id/home_text11"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/home_item_text2top"
                        android:text="智能照片比对"
                        android:textColor="@color/text_gray"
                        android:layout_centerHorizontal="true"
                        />
                </RelativeLayout>


                <RelativeLayout
                    android:layout_toRightOf="@id/home_test_1"
                    android:clickable="true"
                    android:id="@+id/home_test_2"
                    android:layout_width="175dp"
                    android:layout_height="120dp">
