                        android:layout_centerHorizontal="true"
                        />
                    <TextView
                        android:id="@+id/home_text62"
                        android:layout_below="@id/home_text61"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/home_item_text2top"
                        android:text="敬请期待"
                        android:textColor="@color/text_gray"
                        android:layout_centerHorizontal="true"
                        />
                </RelativeLayout>

            </RelativeLayout>

        </RelativeLayout>
