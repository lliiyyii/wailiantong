                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/home_item_imgbottom"
                        android:text="户口本识别"
                        android:textColor="@color/text_black"
                        android:layout_centerHorizontal="true"
                        />
                    <TextView
                        android:id="@+id/home_text52"
                        android:layout_below="@id/home_text51"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/home_item_text2top"
