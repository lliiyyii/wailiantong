                        android:text="证件检测"
                        android:textColor="@color/text_gray"
                        android:layout_centerHorizontal="true"
                        />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_below="@id/home_test_2"
                    android:layout_toRightOf="@+id/home_test_3"
                    android:clickable="true"
                    android:id="@+id/home_test_4"
