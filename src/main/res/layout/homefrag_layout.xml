                        android:layout_below="@id/home_img2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/home_item_imgbottom"
                        android:text="真人与公安库对比"
                        android:textColor="@color/text_black"
                        android:layout_centerHorizontal="true"
                        />
                    <TextView
                        android:id="@+id/home_text22"
                        android:layout_below="@id/home_text21"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/home_item_text2top"
                        android:text="智能身份审核"
                        android:textColor="@color/text_gray"
                        android:layout_centerHorizontal="true"
                        />
                </RelativeLayout>



                <RelativeLayout
                    android:layout_below="@id/home_test_1"
                    android:clickable="true"
                    android:id="@+id/home_test_3"
                    android:layout_width="175dp"
                    android:layout_height="120dp">
